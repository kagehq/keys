{
  "name": "@kagehq/keys",
  "version": "0.6.0",
  "description": "Enterprise permissions layer for AI agents - Scoped, expiring keys with approval workflows, multi-tenancy, and real-time dashboard",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "kage-keys": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "start": "node dist/cli.js start",
    "test": "jest tests/basic.test.ts tests/cli-basic.test.ts tests/working-tests.test.ts tests/comprehensive-api.test.ts --verbose --forceExit --detectOpenHandles --maxWorkers=1",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "prepublishOnly": "npm run clean && npm run build",
    "demo": "node examples/comprehensive-demo.js",
    "mcp-server": "node dist/mcp-server.js",
    "dashboard": "node dist/dashboard-cli.js"
  },
  "keywords": [
    "ai",
    "agents",
    "authentication",
    "authorization",
    "permissions",
    "security",
    "jwt",
    "hmac",
    "broker",
    "proxy",
    "enterprise",
    "multi-tenant",
    "rbac",
    "approval-workflows",
    "dashboard",
    "audit-logging",
    "mcp-server",
    "policy-packs",
    "zero-friction"
  ],
  "author": "Treadie, Inc",
  "license": "FSL-1.1-MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kagehq/keys.git"
  },
  "bugs": {
    "url": "https://github.com/kagehq/keys/issues"
  },
  "homepage": "https://github.com/kagehq/keys#readme",
  "dependencies": {
    "commander": "^11.0.0",
    "sqlite3": "^5.1.6",
    "sqlite": "^5.0.1",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/sqlite3": "^3.1.8",
    "typescript": "^5.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
